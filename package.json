{
  "name": "videoplayer",
  "version": "1.29.4",
  "description": "Custom Video Player on HTML. Used RollupJS, Tailwind with postcss.",
  "author": "markalexi",
  "type": "module",
  "scripts": {
    "start": "node server",
    "lint": "eslint . --ext .js,.mjs,.cjs --ignore-path .gitignore .",
    "lintfix": "eslint . --ext .js,.mjs,.cjs --ignore-path .gitignore . --fix",
    "build": "rollup --config && npx tailwindcss -i ./src/styles/main.css -o ./build/main.css --minify"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "rollup": "^4.13.2",
    "rollup-plugin-eslint": "^7.0.0",
    "tailwindcss": "^3.4.3"
  }
}
